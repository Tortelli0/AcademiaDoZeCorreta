        <TextBox x:Name="txtCep" HorizontalAlignment="Left" Height="19" Margin="112,222,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TabIndex="5" GotFocus="txtCep_GotFocus" LostFocus="txtCep_LostFocus"/>
