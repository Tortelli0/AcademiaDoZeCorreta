        <TextBox x:Name="txtId1" HorizontalAlignment="Left" Height="19" Margin="20,222,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="77" IsEnabled="False"/>
        <Label x:Name="LabelIdLogradouro" Content="{x:Static resources:Idioma.LabelIdLogradouro}" HorizontalAlignment="Left" Height="26" Margin="20,196,0,0" VerticalAlignment="Top" Width="92" Foreground="White"/>

    </Grid>
</Window>
