        <RadioButton x:Name="RdbAtendente" Content="{x:Static resources:Idioma.RdbAtendente}" HorizontalAlignment="Left" Height="17" Margin="650,228,0,0" VerticalAlignment="Top" Width="108" Foreground="White" TabIndex="666666"/>
        <RadioButton x:Name="RdbInstrutor" Content="{x:Static resources:Idioma.RdbInstrutor}" HorizontalAlignment="Left" Height="17" Margin="650,250,0,0" VerticalAlignment="Top" Width="108" Foreground="White" TabIndex="6666666"/>
        <Button x:Name="btn_salvar" Content="{x:Static resources:Idioma.btn_salvar}" HorizontalAlignment="Left" Height="33" Margin="582,367,0,0" VerticalAlignment="Top" Width="176" Click="btn_salvar_Click" TabIndex="14" KeyDown="btn_salvar_KeyDown"/>
