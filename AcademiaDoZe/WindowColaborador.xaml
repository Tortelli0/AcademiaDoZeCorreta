        <xctk:MaskedTextBox Mask="00/00/0000" Margin="25,148,693,281" GotFocus="MaskedTextBox_GotFocus_1" LostFocus="MaskedTextBox_LostFocus_1" TabIndex="3" />

        <xctk:MaskedTextBox Mask="(000) 0000-0000" Margin="284,33,410,392" GotFocus="MaskedTextBox_GotFocus" LostFocus="MaskedTextBox_LostFocus" TabIndex="1" />

